{
  "name": "tsce0",
  "version": "1.0.0",
  "description": "Compose editor and compiler mark 0, in TypeScript",
  "author": "Michael Bayne (http://github.com/samskivert)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/samskivert/compose.git"
  },

  "scripts": {
    "start": "node test/server.js",
    "test": "jest"
  },

  "jest": {
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": ["**/*.test.+(ts|tsx|js)"]
  },

  "dependencies": {
    "mobx": "^5.0.2",
    "mobx-react": "^5.2.3",
    "mobx-react-devtools": "^5.0.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },

  "devDependencies": {
    "@types/jest": "^23.1.0",
    "@types/react": "^16.3.17",
    "@types/react-dom": "^16.0.6",
    "awesome-typescript-loader": "^5.0.0",
    "jest": "^23.1.0",
    "react-addons-test-utils": "^15.6.2",
    "ts-jest": "^22.4.6",
    "typescript": "^2.9.2",
    "webpack": "^4.12.0",
    "webpack-dev-server": "^3.1.4"
  }
}
